#include <bits/stdc++.h>
using namespace std;
int main()
{
    // bitwise and
    int ans = 2 & 3;
    cout << ans << endl;
    int a = 25 & 34;
    cout << a;

    cout << endl;
    // bitwise or
    int o = 23 | 12;
    cout << o << endl;

    // bitwise xor
    int x = 21 ^ 23;
    cout << x << endl;

    // left shift operator
    int l = 23 << 2;
    cout << l << endl;

    // left shift operator
    int r = 34 >> 3;
    cout << r << endl;

    int c = ~23;
    cout<<c;
    return 0;
}
